{
  "name": "server",
  "version": "1.0.0",
  "description": "Server dependencies for leetcode-srs",
  "main": "server.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.1",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.2.1",
    "mongoose": "^5.10.15",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0"
  },
  "scripts": {
    "test": "env TESTING=true mocha ./tests/**/*.test.js --exit --inspect=0.0.0.0:9299 --timeout 120000",
    "_comment": "Note: --exit in the test element is to force the test program to exit after finishing in case we use done()",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix ..\\leetcode-srs",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Steffan Achtmann",
  "license": "MIT"
}
